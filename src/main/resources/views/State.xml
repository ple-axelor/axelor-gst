<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">
	
	<grid name="state-grid" title="State" model="com.axelor.apps.gst.db.State">
		<field name="name"/>
		<field name="country"/>
	</grid>
	
	<form name="state-form" title="State" model="com.axelor.apps.gst.db.State">
		<panel name="state-panel" title="State">
			<field name="name" />
			<field name="country" widget="SuggestBox" canNew="true" form-view="country-form" />
		</panel>
	</form>
	
	</object-views>